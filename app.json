{
  "name": "scaffold",
  "scripts": {},
  "env": {
    "DJANGO_ALLOWED_HOSTS": {
      "required": true,
      "value": ".herokuapp.com"
    },
    "DJANGO_DEBUG": {
      "required": true
    },
    "DJANGO_SECRET_KEY": {
      "required": true,
      "generator": "secret"
    },
    "TLS_ENABLED": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [],
  "stack": "container"
}
